  "keywords": [
    "materialize",
    "materialize-css",
    "material-design",
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^7.1.3",
    "ember-cli-htmlbars": "^5.0.0",
    "rsvp": "^4.7.0"
  },
