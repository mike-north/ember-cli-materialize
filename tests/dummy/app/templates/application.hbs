<header>
  {{#materialize-navbar name='ember-cli-materialize'}}
    <li>{{#link-to 'buttons'}}Buttons{{/link-to}}</li>
    <li>{{#link-to 'navbar'}}Navbar{{/link-to}}</li>
    <li>{{#link-to 'copyright'}}Copyright{{/link-to}}</li>
    <li>{{#link-to 'cards'}}Cards{{/link-to}}</li>
    <li>{{#link-to 'collapsible'}}Collapsible{{/link-to}}</li>
    <li>{{#link-to 'forms'}}Form{{/link-to}}</li>
    <li>{{#link-to 'tabs'}}Tabs{{/link-to}}</li>
    <li>{{#link-to 'badges'}}Badges{{/link-to}}</li>
  {{/materialize-navbar}}
</header>

<main>
  {{outlet}}
</main>

<footer class="page-footer">
    <div class="container">
        <div class="row">
            <div class="col l6 s12">
                <h5 class="white-text">Footer Content</h5>
                <p class="grey-text text-lighten-4">You can use rows and columns here to organize your footer content.</p>
            </div>
            <div class="col l4 offset-l2 s12">
                <h5 class="white-text">Links</h5>
                <ul>
                    <li><a class="grey-text text-lighten-3" href="#!">Link 1</a></li>
                    <li><a class="grey-text text-lighten-3" href="#!">Link 2</a></li>
                    <li><a class="grey-text text-lighten-3" href="#!">Link 3</a></li>
                    <li><a class="grey-text text-lighten-3" href="#!">Link 4</a></li>
                </ul>
            </div>
        </div>
    </div>
  {{#materialize-copyright text='Copyright Text' startYear=2014}}<a class="grey-text text-lighten-4 right" href="#!">More Links</a>{{/materialize-copyright}}
</footer>
