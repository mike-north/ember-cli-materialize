<thead>
  <tr>
    {{#each headerColumns as |column|}}
      {{view 'ember-cli-materialize@materialize-table-header-cell'
        column=column
        width=column.width}}
    {{/each}}
  </tr>
</thead>
<tbody>
  {{#each content as |item|}}
    {{view 'ember-cli-materialize@materialize-table-row-cells'
      table=this
      model=item
      content=columns}}
  {{/each}}
</tbody>
{{yield}}
